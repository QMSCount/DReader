<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:layout_gravity="bottom" android:background="@color/general__shared__ffffff"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:orientation="horizontal"
        android:id="@id/surfing__surfing_navigate_view__tab" android:layout_width="fill_parent"
        android:layout_height="fill_parent">
        <LinearLayout android:orientation="vertical"
            android:id="@id/surfing__surfing_navigate_view__recommend" android:layout_width="0.0dip"
            android:layout_height="fill_parent" android:layout_weight="1.0">
            <ImageView android:paddingTop="5.0dip" android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/surfing__surfing_tab_view__store"
                android:scaleType="center" />
            <TextView android:textSize="@dimen/general_font__shared__e"
                android:textColor="@color/general__shared__666666_selected" android:gravity="center"
                android:paddingTop="3.0dip" android:paddingBottom="3.0dip"
                android:layout_width="fill_parent" android:layout_height="wrap_content"
                android:text="@string/general__shared__store" />
        </LinearLayout>
        <LinearLayout android:orientation="vertical"
            android:id="@id/surfing__surfing_navigate_view__category" android:layout_width="0.0dip"
            android:layout_height="fill_parent" android:layout_weight="1.0">
            <ImageView android:paddingTop="5.0dip" android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/surfing__surfing_tab_view__category"
                android:scaleType="center" />
            <TextView android:textSize="@dimen/general_font__shared__e"
                android:textColor="@color/general__shared__666666_selected" android:gravity="center"
                android:paddingTop="3.0dip" android:paddingBottom="3.0dip"
                android:layout_width="fill_parent" android:layout_height="wrap_content"
                android:text="@string/general__shared__category" />
        </LinearLayout>
        <LinearLayout android:orientation="vertical"
            android:id="@id/surfing__surfing_navigate_view__bookshelf" android:layout_width="0.0dip"
            android:layout_height="fill_parent" android:layout_weight="1.0">
            <ImageView android:paddingTop="5.0dip" android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/surfing__surfing_tab_view__bookshelf"
                android:scaleType="center" />
            <TextView android:textSize="@dimen/general_font__shared__e"
                android:textColor="@color/general__shared__666666_selected" android:gravity="center"
                android:paddingTop="3.0dip" android:paddingBottom="3.0dip"
                android:layout_width="fill_parent" android:layout_height="wrap_content"
                android:text="@string/general__shared__bookshelf" />
        </LinearLayout>
        <FrameLayout android:layout_width="0.0dip" android:layout_height="fill_parent"
            android:layout_weight="1.0">
            <LinearLayout android:orientation="vertical"
                android:id="@id/surfing__surfing_navigate_view__personal"
                android:layout_width="fill_parent" android:layout_height="fill_parent">
                <ImageView android:paddingTop="5.0dip" android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:src="@drawable/surfing__surfing_tab_view__personal"
                    android:scaleType="center" />
                <TextView android:textSize="@dimen/general_font__shared__e"
                    android:textColor="@color/general__shared__666666_selected"
                    android:gravity="center" android:paddingTop="3.0dip"
                    android:paddingBottom="3.0dip" android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/general__shared__personal" />
            </LinearLayout>
            <ImageView android:layout_gravity="center"
                android:id="@id/surfing__surfing_navigate_view__personal_message_count_image"
                android:visibility="invisible" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_marginLeft="10.0dip"
                android:layout_marginBottom="15.0dip"
                android:src="@drawable/bookshelf__header_view__have_new_message" />
            <ImageView android:layout_gravity="center"
                android:id="@id/surfing__surfing_navigate_view__personal_cart_count_image"
                android:visibility="invisible" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_marginLeft="10.0dip"
                android:layout_marginBottom="15.0dip"
                android:src="@drawable/bookshelf__header_view__have_new_message" />
            <ImageView android:layout_gravity="center"
                android:id="@id/surfing__surfing_navigate_view__personal_task_count_image"
                android:visibility="invisible" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_marginLeft="10.0dip"
                android:layout_marginBottom="15.0dip"
                android:src="@drawable/bookshelf__header_view__have_new_message" />
        </FrameLayout>
    </LinearLayout>
    <View android:background="#ffdddddd" android:layout_width="fill_parent"
        android:layout_height="1.0px" />
</FrameLayout>