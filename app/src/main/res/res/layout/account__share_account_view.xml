            <View android:background="@color/general__shared__bcbcbc"
                android:layout_width="fill_parent" android:layout_height="1.0px" />
            <com.duokan.reader.ui.general.DkCheckBox android:orientation="horizontal"
                android:id="@id/personal__personal_settings_view__sync_evernote"
                android:layout_width="fill_parent" android:layout_height="60.0dip">
                <RelativeLayout android:layout_width="fill_parent" android:layout_height="60.0dip">
                    <ImageView android:id="@id/personal__personal_settings_view__sync_evernote_icon"
                        android:layout_width="wrap_content" android:layout_height="wrap_content"
