                android:layout_gravity="center|left" android:paddingTop="35.0dip"
                android:paddingBottom="10.0dip" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:text="@string/account_sina" />
            <View android:background="@color/general__shared__bcbcbc"
                android:layout_width="fill_parent" android:layout_height="1.0px" />
            <LinearLayout android:gravity="center|left" android:id="@id/account_sina_ll"
