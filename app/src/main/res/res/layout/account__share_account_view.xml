<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:background="@color/general__shared__ffffff"
    android:layout_width="fill_parent" android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <com.duokan.reader.ui.general.PageHeaderView
        android:id="@id/account__share_account_view__header" android:layout_width="fill_parent"
        android:layout_height="wrap_content" />
    <ScrollView android:background="@android:color/white" android:layout_width="fill_parent"
        android:layout_height="fill_parent">
        <LinearLayout android:orientation="vertical" android:paddingLeft="15.0dip"
            android:paddingRight="15.0dip" android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <com.duokan.reader.ui.general.DkLabelView
                android:textSize="@dimen/general_font__shared__d"
                android:textColor="@color/general__shared__888888"
                android:layout_gravity="center|left" android:paddingTop="25.0dip"
                android:paddingBottom="10.0dip" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:text="@string/account_yingxiang" />
            <View android:background="@color/general__shared__bcbcbc"
                android:layout_width="fill_parent" android:layout_height="1.0px" />
            <LinearLayout android:orientation="horizontal" android:id="@id/account_yingxiang_ll"
                android:layout_width="fill_parent" android:layout_height="60.0dip">
                <ImageView android:layout_gravity="center|left" android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/general__shared__yingxiang_icon_list" />
                <com.duokan.reader.ui.general.DkLabelView
                    android:textSize="@dimen/general_font__shared__c"
                    android:textColor="@color/general__shared__333333"
                    android:layout_gravity="center|left" android:id="@id/account_yingxiang"
                    android:layout_width="wrap_content" android:layout_height="wrap_content"
                    android:layout_marginLeft="15.0dip"
                    android:text="@string/account_bind_yingxiang" />
